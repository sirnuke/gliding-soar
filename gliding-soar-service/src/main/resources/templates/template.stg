bundle(model, arguments, version, when, elements) ::= <<
# Glide <version>
# Generated on <when>
#
<if(arguments)># Arguments: <arguments><endif>
#
# Autogenerated code, do not modify

<elements:element(); separator="\n">
>>

element(e) ::= <<
<namespace(e, e.identifier.namespace
>>

namespace(e, names) ::= <<
<if(names)>
namespace eval <first(names).value> {
    <namespace(e, rest(names))>
}
<else>
namespace eval <e.identifier.value> {
    <body(e)>
}
<endif>
>>

