import "common.stg"

output(element) ::= "<namespace(element.identifier.value, body(element))>"

body(element) ::= <<
<ngsDeclareType(element)>
<declareIdentifier(element)>
<declareType(element)>
<declareMembers(element)>

<bindWrapper(element)>
<modifyWrapper(element)>
<constructWrapper(element)>
<constructSetWrapper(element)>
<deployWrapper(element)>
>>

