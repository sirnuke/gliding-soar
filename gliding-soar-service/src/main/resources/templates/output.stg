import "common.stg"

object(element) ::= "<namespace(element.identifier.value, body(element))>"

body(element) ::= <<
<ngsDeclareType(element)>
<declareIdentifier(element)>
<declareType(element)>

<bindWrapper(element)>
<setWrapper(element)>
<applyWrapper(element)>
<addToWrapper(element)>
<removeWrapper(element)>
<constructWrapper(element)>
<constructSetWrapper(element)>
<initiateWrapper(element)>
<deployWrapper(element)>
>>

